{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  
  "bunVersion": "1.x",

  "framework": "elysia",

  "installCommand": "bun install",
  "buildCommand": "bun run build",
  "outputDirectory": "dist",

  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}
